{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "modules": false
    }],
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-class-properties",
    ["transform-es2015-modules-commonjs-simple", {
      "noMangle": true
    }],
    "lodash"
  ]
}
